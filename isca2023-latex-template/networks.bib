@inproceedings{Souza:86,
  AUTHOR =	 {Robert J. Souza and Steven P. Miller},
  TITLE =	 "{Unix and Remote Procedure Calls: a Peaceful
                  Coexistence?}",
  BOOKTITLE =	 {Proc. of the 6th International Conference on
                  Distributed Computing Systems},
  YEAR =	 {1986},
  PAGES =	 {268--277},
  ORGANIZATION = {IEEE},
  ADDRESS =	 {Boston, Mass.},
  MONTH =	 MAY,
  ABSTRACT =	 "This paper describes a prototype remote procedure
                  call system for the 4.2 BSD UNIX operating
                  system. The benefits of RPC as a mechanism for
                  building distributed applications have received much
                  attention. Several implementations of RPC systems
                  are reported in the literature; others are under
                  construction. Our implementation differs in that its
                  design was subject to several a priori constraints,
                  both technical and strategic. First, we did not make
                  any modifications to the UNIX kernel. Second, we
                  hoped to be able to port existing applications to
                  use the RPC. Third, we wanted to use and support a
                  particular language suite. Nonetheless, we conclude
                  that it is feasible to build an RPC system in a UNIX
                  environment, and have successfully converted
                  existing applications to use the RPC system. This
                  paper describes how the UNIX language suite,
                  networking, and process structure influenced the
                  design and constrain the applicability of remote
                  procedure calls."
}

@inproceedings{Spector:81,
  AUTHOR =	 "A. Z. Spector",
  TITLE =	 "{Performing Remote Operations Efficiently on a Local
                  Computer Network}",
  BOOKTITLE =	 "Proceedings of the Eighth Symposium on Operating
                  Systems Principles",
  YEAR =	 1981,
  ADDRESS =	 "Asilomar",
  ABSTRACT =	 "A communication model is described that can serve as
                  a basis for a highly efficient communication
                  subsystem for local networks. The model contains a
                  taxonomy of communication instructions that can be
                  implemented efficiently and can be a good basis for
                  interprocessor communication. These communication
                  instructions, called remote references, cause an
                  operation to be performed by a remote process and,
                  optionally, cause a value to be returned. This paper
                  also presents implementation considerations for a
                  communication system based upon the model and
                  describes an experimental communication subsystem
                  that provides one class of remote references. These
                  remote references take about 150 microseconds or 50
                  average instruction times to perform on Xerox Alto
                  computers connected by a 2.94 megabit Ethernet."
}

