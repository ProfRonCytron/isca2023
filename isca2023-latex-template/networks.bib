@inproceedings{Omalley:90a,
  AUTHOR =	 "Sean W. O'Malley and Larry L. Peterson",
  TITLE =	 "{A Highly Layered Architecture for High-Speed
                  Networks}",
  BOOKTITLE =	 "Proceedings of the 2nd International Workshop on
                  High-Speed Networks",
  MONTH =	 nov,
  YEAR =	 1990,
  ANNOTE =	 "Read on Dec 9th, 1991",
  ABSTRACT =	 "This paper describes an architecture for high-speed
                  networks. In contrast to other proposals that argue
                  that layering is the bane of efficiency, this
                  approach is based on a highly layered architecture,
                  in fact, one with more layers than even conventional
                  network architectures. The key justification for
                  multiple layers is that it provides application
                  programmers with the most flexibility in selecting
                  exactly the right functionality for their
                  application. In addition to presenting the
                  architecture, the paper also describes several
                  aspects of its implementation, including use of a
                  new technique called lazy messages, that allow the
                  architecture to achieve the same performance
                  advantages as architectures that have abandoned
                  layers."
}

@techreport{Omalley:90b,
  AUTHOR =	 "Sean W. O'Malley and Larry L. Peterson",
  TITLE =	 "{A New Methodology for Designing Network Software}",
  NUMBER =	 "TR 90-29",
  INSTITUTION =	 "Department of Computer Science, University of
                  Arizona",
  ADDRESS =	 "Tucson, Ariz.",
  MONTH =	 aug,
  YEAR =	 1990,
  ABSTRACT =	 "Network software is a critical component of any
                  distributed system. Because of its complexity,
                  network software is commonly layered into a
                  hierarchy of protocols, or more generally, into a
                  protocol graph. Typical protocol graphs, including
                  those standardized in the ISO and TCP/IP network
                  architectures, share three important properties: the
                  protocol graph is simple, the nodes of the graph
                  (protocols) encapsulate complex functionality, and
                  the topology of the graph is relatively static. This
                  paper describes a new methodology for organizing
                  network software that differs from conventional
                  architectures in all three of these properties. In
                  this approach, the protocol graph is complex,
                  individual protocols encapsulate a single function,
                  and the topology of the graph is dynamic. The main
                  contribution of this paper is to present the
                  methodology, illustrate its advantages, and
                  demonstrate that it results in efficient network
                  software."
}

@article{Omalley:90c,
  AUTHOR =	 "Sean W. O'Malley and Mark B. Abbott and Norman
                  C. Hutchinson and Larry L. Peterson",
  TITLE =	 "{A Transparent Blast Facility}",
  JOURNAL =	 "Journal of Internetworking",
  MONTH =	 dec,
  VOLUME =	 1,
  NUMBER =	 2,
  YEAR =	 1990,
  ANNOTE =	 "Read 12/20/91 (ds)",
  ABSTRACT =	 "A transparent blast facility provides a flexible
                  solution to the problem of achieving high
                  user-to-user throughput in a heterogeneous
                  network. The facility achieves high throughput by
                  avoiding the cost of copying large messages across
                  the user/kernel boundary. The facility is
                  transparent in that it simultaneously supports a
                  wide variety of protocols, it is not embedded in or
                  restricted to any one protocol. This paper describes
                  the kernel mechanisms and algorithms that make up
                  the transparent blast facility. It also shows that
                  for several RPC protocols, the facility supports
                  effective user-to-user throughput rates to within 87
                  percent of kernel-to-kernel throughput rates."
}

