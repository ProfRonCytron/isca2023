@inproceedings{Zhang:90,
  AUTHOR =	 "Xi Zhang and Aruna Seneviratne",
  TITLE =	 "{An Efficient Implementation of High-Speed Protocol
                  without Data Copying}",
  BOOKTITLE =	 "{Proceedings of the $15^{th}$ Conference on Local
                  Computer Networks}",
  ORGANIZATION = "IEEE",
  ADDRESS =	 "Minneapolis, MN",
  YEAR =	 1990,
  MONTH =	 oct,
  PAGES =	 "443--450",
  ABSTRACT =	 "In this paper an approach of implementing a method
                  of minimizing data copying, called Buffer Cut
                  Through is proposed. It uses the mechanism of
                  upcalls. It enables data passing through layers
                  without copying, relying on the principle of passing
                  responsibility for the buffer from one layer to
                  another. Data is shared among several layers. These
                  buffers are divided into a header buffer and a data
                  buffer. When packet is received, the transport
                  protocol needs to create the transport header. No
                  data copying or data buffer handling is
                  required. Implementation of the BCT in TCP/IP is
                  described and the next step is to perform
                  simulations on the system."
}

@inproceedings{Zhang:90b,
  AUTHOR =	 "Lixia Zhang",
  TITLE =	 "{Virtual Clock: A New Traffic Control Algorithm for
                  Packet Switched Networks}",
  BOOKTITLE =	 sigcomm,
  ORGANIZATION = "ACM",
  ADDRESS =	 "Philadelphia, PA",
  YEAR =	 1990,
  MONTH =	 sep,
  PAGES =	 "19--29",
  ABSTRACT =	 "A challenging research issue in high speed
                  networking is how to control the transmission rate
                  of statistical data flows. This paper describes a
                  new algorithm, VirtualClock, for data traffic
                  control in high-speed networks. VirtualClock
                  maintains the statistical multiplexing flexibility
                  of packet switching while ensuring each data flow
                  its reserved average throughput rate at the same
                  time. The algorithm has been tested through
                  simulation."
}

