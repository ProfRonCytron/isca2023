@inproceedings{Ousterhout:90a,
  AUTHOR =	 {J. K. Ousterhout},
  TITLE =	 "{Why Aren't Operating Systems Getting Faster As Fast
                  as Hardware?}",
  YEAR =	 1990,
  MONTH =	 jun,
  PUBLISHER =	 "USENIX Association",
  BOOKTITLE =	 {USENIX Summer Conference},
  PAGES =	 {247--256},
  ABSTRACT =	 "This paper evaluates several hardware platforms and
                  operating systems using a set of benchmarks that
                  stress kernel entry/exit, file systems, and other
                  things related to operating systems. The overall
                  conclusion is that operating system performance is
                  not improving at the same rate as the base speed of
                  the underlying hardware. The most obvious ways to
                  remedy this situation are to improve memory
                  bandwidth and reduce operating systems' tendency to
                  wait for disk operations to complete."
}

