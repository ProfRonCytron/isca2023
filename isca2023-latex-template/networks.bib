@inproceedings{Almes:86,
  AUTHOR =	 {Guy T. Almes},
  TITLE =	 "{The Impact of Language and System on Remote
                  Procedure Call Design}",
  BOOKTITLE =	 {Proc. of the 6th International Conference on
                  Distributed Computing Systems},
  YEAR =	 {1986},
  PAGES =	 {414--421},
  ORGANIZATION = {IEEE},
  ADDRESS =	 {Boston, Mass.},
  MONTH =	 MAY,
  ABSTRACT =	 "The technique of remote procedure calls is emerging
                  as a popular approach to communication in locally
                  distributed computations. The general idea of RPC --
                  to provide an efficient and robust mechanism
                  faithful to the semantics of ordinary procedure
                  calls in a given language -- has great appeal. The
                  advantages of actually using the RPC approach in a
                  particular language-system context, however, can
                  depend dramatically on the particular language and
                  system used. In this paper, we describe one such RPC
                  system, based upon Module-2 and the V operating
                  system. The paper focuses on how particular aspects
                  of our language and system context influenced the
                  nature and quality of the resulting system. The
                  lessons learned have implications both for others
                  contemplating RPC designs and for designers of
                  languages and operating system."
}

