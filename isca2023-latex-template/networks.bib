}

@inproceedings{Schwartz:90,
  AUTHOR =	 "Jiraj Jain and Mischa Schwartz and Theodore Bashkow",
  TITLE =	 "{Transport Protocol Processing at GBPS Rates}",
  BOOKTITLE =	 sigcomm,
  ORGANIZATION = "ACM",
  ADDRESS =	 "Philadelphia, PA",
  YEAR =	 1990,
  MONTH =	 sep,
  PAGES =	 "188--199",
  ANNOTE =	 "read 12/22/91 (ds)",
  ABSTRACT =	 "This paper proposes an architecture for
                  accomplishing transport protocol processing at Gbps
                  rates. The limitations of currently used transport
                  protocols have been analyzed extensively in recent
                  literature. Several benchmark studies have
                  established the achievable throughput of ISO TP4 and
                  TCP to be in the low Mbps range; several new
                  protocols and implementation techniques have been
                  proposed to achieve 100 Mbps and higher throughput
                  rates. We briefly review some of these protocols and
                  establish the need for a radically different
                  approach to meet our objective. An estimate of the
                  aggregate processing power required for Gbps
                  throughput is developed. It is proposed that a cost
                  effective and practical solution to the processing
                  requirements could be based on a multi-processor
                  system. The opportunities for parallel processing in
                  a typical transport protocol are examined and
                  arguments are advanced for selecting a favored
                  approach. A corresponding parallel processing
                  architecture is described. Data structures used to
                  preserve packet ordering and techniques for reducing
                  contention in a multi-processing environment are
                  discussed. An implementation methodology for
                  conventional transport protocols is outlined. Some
                  suggestions are made for improving efficiency by
                  making modifications to the protocol that do not
                  compromise functionality. The performance achievable
                  with this modified architecture is analyzed and some
                  suggestions for further work are presented."
}

@article{Schwartz:91,
  AUTHOR =	 "Thomas F. La Porta and Mischa Schwartz",
  TITLE =	 "{Architectures, Features, and Implementation of
                  High-Speed Transport Protocols}",
  JOURNAL =	 "IEEE Network Magazine",
  YEAR =	 1991,
  MONTH =	 may,
  PAGES =	 "14--22",
  ANNOTE =	 "Read 12/20/91 (ds)",
  ABSTRACT =	 "As computer communications advance further into the
                  era of lightwave networks, more strain is placed on
                  data communication components. One component
                  receiving much recent attention is the transport
                  layer, the fourther layer of the OSI model of
                  communication protocols. There has been significant
                  debate whether current transport protocols ({\em
                  e.g.}, Transmission Control Protocol (TCP) or
                  International Standards Organization (ISO) TP4) can
                  adequately handle the burden placed upon them by
                  newly developing and futuristic high-speed
                  networks. Alternatives such as the development of
                  new transport protocols or new protocol
                  architectures have been proposed. Lightwave networks
                  currently being introduced ({\em e.g.}, Fiber
                  Distributed Data Interface -- FDDI) supply much
                  higher bandwidth to users than current
                  networks. Future networks are expected to be able to
                  supply users with gigabit capacity, which is several
                  orders of magnitude above what current networks
                  offer. The ability of current transport protocols to
                  support these data rates has been questioned. In
                  addition, lightwave networks will supply much more
                  reliable service, with bit error rates as low as
                  $10^{-9}$ compared to networks today with bit error
                  rates of $10^{-4}$. How transport protocols may take
                  advantage of this improvement has also been
                  examined."
}

@inproceedings{Schwartz:92,
  AUTHOR =	 "Thomas F. La Porta and Mischa Schwartz",
  TITLE =	 "{Design, Verification, and Analysis of a High Speed
                  Protocol Parallel Implementation Architecture}",
  BOOKTITLE =	 "Proceedings of the First IEEE Workshop on the
                  Architecture and Implementation of High Performance
                  Communication Subsystems",
  MONTH =	 feb,
  YEAR =	 1992,
  ABSTRACT =	 "This paper presents a detailed architecture for the
                  parallel implementation of a feature-rich transport
                  protocol. While the architecture presented is
                  designed for a specific protocol, its use is not
                  restricted to a single protocol, or protocol
                  layer. The design is formulated by dividing general
                  protocol functions, such as connection management or
                  reliable data delivery, into subtasks which can be
                  performed in parallel. Some of these tasks are
                  performed by a dedicated process, and others are
                  distributed over several processes. The target
                  transport protocol supports several modes of
                  operation, including various acknowledgement,
                  reject, and retransmission schemes, making it
                  extremely flexible. The protocol user may instruct
                  the protocol to dynamically change modes
                  ``on-the-fly'' without loss of data. In addition,
                  other protocols may be implemented using this
                  architecture by programming their protocol rules
                  into the architecture. Protocol specific state
                  machines and structures are plugged into this
                  architecture to define packet validity, packet
                  formats, etc. This architecture supports 3-way
                  handshake or implicit connection establishment
                  procedures, 2-way handshake connection termination
                  procedures, and reliable or unreliable data
                  transmission and reception. The operation of this
                  architecture has been validated and verified using
                  the Coordination-Specification Analyzer
                  (COSPAN). This validation and verification proves
                  that the architecture opens and closes connections
                  properly, and provides reliable data transmission
                  and reception. No deadlocks are present within this
                  architecture. Analysis of a possible TCP
                  implementation using this architecture demonstrates
                  that the architecture developed here is efficient in
                  that the protocol processing is divided equitably
                  among the parallel protocol machines. Performance
                  analysis shows that the parallel architecture may
                  supply up to 60 percent higher throughput than a
                  serial implementation of TCP."
}

