@article{RSS:90,
  AUTHOR =	 "Krithi Ramamritham and John~A. Stankovic and
                  Perng-Fei Shiah",
  TITLE =	 "{Efficient Scheduling Algorithms for Real-time
                  Multiprocessor Systems}",
  JOURNAL =	 "IEEE Transactions on Parallel and Distributed
                  Systems",
  MONTH =	 apr,
  YEAR =	 1990,
  VOLUME =	 1,
  NUMBER =	 2,
  PAGES =	 "184--194",
  ABSTRACT =	 "Describes and evaluates two scheduling algorithms
                  based on heuristic functions. Both incrementally
                  construct a schedule by iteratively adding one task
