@inproceedings{Schutt:92,
  AUTHOR =	 "Manny Farber and Thomas Schutt",
  TITLE =	 "{High-Speed Protocol Implementations: An Experience
                  with PASS}",
  BOOKTITLE =	 "Proceedings of the First IEEE Workshop on the
                  Architecture and Implementation of High Performance
