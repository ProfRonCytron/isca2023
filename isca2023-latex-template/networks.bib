@article{Schmidt:92a,
  AUTHOR =	 "Douglas C. Schmidt and Tatsuya Suda",
  TITLE =	 "{Transport System Architecture Services for
                  High-Performance Communications Systems}",
  JOURNAL =	 "IEEE Journal on Selected Areas in Communication",
  MONTH =	 may,
  YEAR =	 1993,
  VOLUME =	 11,
  NUMBER =	 4,
  PAGES =	 "489-506",
  ABSTRACT =	 "Providing end-to-end gigabit communication support
                  for high-bandwidth multimedia applications requires
                  transport systems that transfer data efficiently via
                  network protocols such as TCP, TP4, XTP, and
                  ST-II. This paper describes and classifies transport
                  system services that integrate operating system
                  resources such as CPU(s), virtual memory, and I/O
                  devices together with network protocols to support
                  distributed multimedia applications running on local
                  and wide area networks. A taxonomy is presented that
                  compares and evaluates four commercial and
                  experimental transport systems in terms of their
                  protocol processing support. The systems covered in
                  this paper include System V UNIX, BSD UNIX, the
                  x-kernel, and Choices Conduit. This paper is
                  intended to navigate system researchers and
                  developers through the transport system design space
                  by describing various transport system architecture
                  alternatives."
}

@techreport{Schmidt:92b,
  AUTHOR =	 "Andrew Schmidt and Roy Campbell",
  TITLE =	 "{Internet Protocol Traffic Analysis with
                  Applications for ATM Switch Design}",
  INSTITUTION =	 "University of Illinois at Urbana-Champaign",
  NUMBER =	 "TR92--1735",
  YEAR =	 1992,
  ABSTRACT =	 "This paper concerns the analysis of networks and how
                  their communication patterns can be applied to the
                  design of a virtual circuit packet switch. The
                  future of Asynchronous Transfer Mode (ATM) networks
                  depends on the design and development of virtual
                  circuit packet switches. The performance
                  requirements of ATM switches can be estimated by
                  measurements of current data communication
                  networks. Previous network studies do not provide an
                  adequate model of conversations between hosts, an
                  issue of relevance to circuit establishment in
                  virtual circuit packet switches. Virtual circuit
                  switches route a conversation consisting of
                  sequences of packets along a unique path through a
                  network. A model for such data conversations can be
                  determined from data traffic using a
                  connection-oriented protocol in existing
                  networks. However, many of the network services
                  offered on current networks depend on a
                  connectionless protocol. A complete data
                  conversation model must account for data traffic
                  using both protocols. In this study, we measure the
                  behavior of both local and wide area networks and
                  estimate the behavior of a virtual circuit network
                  carrying the same data traffic."
}

@inproceedings{Schmidt:92c,
  AUTHOR =	 "Douglas C. Schmidt and Donald F. Box and Tatsuya
                  Suda",
  TITLE =	 "{ADAPTIVE: A Flexible and Adaptive Transport System
                  Architecture to Support Lightweight Protocols for
                  Multimedia Applications on High-Speed Networks}",
  BOOKTITLE =	 "Proceedings of the $1^{st}$ Symposium on
                  High-Performance Distributed Computing (HPDC-1)",
  MONTH =	 sep,
  PAGES =	 "174-186",
  ORGANIZATION = {IEEE},
  ADDRESS =	 {Syracuse, New York},
  YEAR =	 1992,
  ABSTRACT =	 "Existing transport systems do not specially-tailor
                  their services to meet the communication
                  requirements of distributed applications. This often
                  forces developers into the procrustean framework
                  offered by transport systems and protocols that were
                  designed before the advent of multimedia
                  applications and high-speed networks. Flexible and
                  adaptive transport systems, on the other hand,
                  dynamically configure lightweight protocols that
                  meet application requirements more precisely. This
                  paper describes a high-performance transport system
                  architecture called ADAPTIVE, ``A Dynamically
                  Assembled Protocol Transformation, Integration, and
                  Validation Environment.'' We are developing ADAPTIVE
                  to support multimedia applications running on
                  high-speed networks. ADAPTIVE is a transformational
                  system providing policies and mechanisms that
                  automatically specify and configure a flexible and
                  adaptive transport system. In addition, it provides
                  a controlled prototyping environment for monitoring,
                  analyzing, and experimenting with the effects of
                  different transport system designs and
                  implementations on application performance."
}

