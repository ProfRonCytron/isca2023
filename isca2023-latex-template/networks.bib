@article{Gerber:95,
  AUTHOR =	 "Richard Gerber and William Pugh and Manas Saksena",
  TITLE =	 "{Parametric Dispatching of Hard Real-time Tasks}",
  JOURNAL =	 "{IEEE Transactions on Computers}",
  MONTH =	 mar,
  YEAR =	 1995,
  VOLUME =	 44,
  NUMBER =	 3,
  Review =	 "Introduces the technique of {\em parametric
                  dispatching} to enforce relative timing
                  constraints. It is intended for use in statically
                  scheduled, non-preemptive systems. The relative
                  timing constraints must provide a total ordering on
                  the tasks. In contrast to other approaches, however,
                  worst case execution times need not be
                  provided. Instead, upper and lower bounds on
                  execution times are used. Both on-line and off-line
                  components are included in parametric
                  dispatching. The off-line component analyzes the
                  timing constraints and generates a calendar of
                  functions which yield the upper and lower bounds of
                  each task. The on-line scheduling component
                  evaluates these functions and provides the bounds to
                  the dispatcher."
}

