@techreport{Renesse:86,
  AUTHOR =	 "R. van Renesse and Staveren",
  TITLE =	 "{Wide-Area Communication under Amoeba}",
  INSTITUTION =	 "Department of Mathematics and Computer Science,
                  Vrije Universiteit",
