@article{Mehra:97,
  AUTHOR =	 "Ashish Mehra and Atri Indiresan and Kang~G. Shin",
  TITLE =	 "{Structuring Communication Software for
                  Quality-of-Service Guarantees}",
  JOURNAL =	 "IEEE Transactions on Software Engineering",
  MONTH =	 oct,
  YEAR =	 1997,
  VOLUME =	 23,
  NUMBER =	 10,
  PAGES =	 "616--634",
  REVIEW =	 "Describes a communication subsystem architecture
                  that provides admission control, traffic
                  enforcement, buffer management, and CPU and link
                  scheduling. The architecture maintains QoS
                  guarantees on a per-connection basis, provides
                  overload connection between established connections,
                  and offers fairness to best-effort traffic. The
                  architecture is used to implement {\em Real-time
                  channels}, for communication in packet-switched
                  networks. A channel handler is associated with each
                  channel; the handlers execute using cooperative
                  preemption."
}

@article{Stewart:97,
