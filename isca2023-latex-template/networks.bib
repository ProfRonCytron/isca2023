@inproceedings{Peterson:92a,
  AUTHOR =	 {Mark B. Abbott and Larry L. Peterson},
  TITLE =	 "{A Language-Based Approach to Protocol
                  Implementation}",
  BOOKTITLE =	 sigcomm,
  ORGANIZATION = "ACM",
  ADDRESS =	 "Baltimore Maryland",
  YEAR =	 1992,
  ABSTRACT =	 "Morpheus is a special-purpose programming language
                  that facilitates the efficient implementation of
                  communication protocols. Protocols are divided into
                  three categories so that they can inherit code and
                  data structures based on their category; the
                  programmer implements a particular protocol by
                  refining the inherited structures. Morpheus
                  optimization techniques reduce per-layer overhead on
                  time-critical operations to a few assembler
                  instructions even though the protocol stack is not
                  determined until runtime. This supports
                  divide-and-conquer simplification of the programming
                  task by minimizing the penalty for decomposing
                  complex protocols into combinations of simpler
                  protocols."
}

@inproceedings{Peterson:92b,
  AUTHOR =	 "Mary L. Bailey and Michael A. Pagels and Larry
                  L. Peterson",
  TITLE =	 "{The {\em x}-chip: An Experiment in Hardware
                  Demultiplexing}",
  BOOKTITLE =	 "Proceedings of the First IEEE Workshop on the
                  Architecture and Implementation of High Performance
                  Communication Subsystems",
  MONTH =	 feb,
  YEAR =	 1992,
  ABSTRACT =	 "An important issue in the design of host-network
                  interfaces is how much protocol processing should be
                  done is hardware ({\em i.e.}, on the network device)
                  and how much should be done in software ({\em i.e.},
                  on the host). This issue is becoming increasingly
                  important as improving network performance puts more
                  and more stress on the end host. As a result, there
                  have recently been several efforts to experiment
                  with different points in the design spectrum: (1)
                  Nectors communications adaptor board (CAB); (2)
                  Aurora gigabit network testbed; (3) Network Adaptor
                  Board. Each of these architectures implements one or
                  more protocols on the network device -- in the case
                  of CAB it is an entire protocol suite; in the case
                  of NAB, it is a single high-level transport
                  protocol, and in the case of Aurora, it is a single
                  low-level protocol. This paper introduces another
                  point in the design spectrum: the {\em
                  x}-chip. Rather than implement one or more protocols
                  on the network device, the x-chip implements a
                  collection of protocol-independent functions on the
                  network device. The x-chip does not replace the
                  software implementations of protocols; it just
                  provides hardware assist to these protocols. The
                  functions provided are simple enough that they can
                  be implemented by specialized chips; general purpose
                  processors are not used in the design. The x-chip is
                  an outgrowth of our work on the x-kernel -- a
                  sofware architecture that supports protocol
                  implementations. one of the design philosophies of
                  the x-kernel has been to identify tasks that are
                  common to all protocols, and to provide general
                  tools that support these tasks. The x-chip takes
                  this philosophy one step further by moving certain
                  aspects of the support tools into hardware, with the
                  ultimate goal of vertically integrating the network
                  device and the network software infrastructure."
}

