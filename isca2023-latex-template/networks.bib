@techreport{BLINKBLT:90a,
  AUTHOR =	 {ANSI},
  TITLE =	 "{Compatability of BLINKBLT with the ATM adaptation
                  layer}",
  NUMBER =	 {T1S1.5/90-009},
  INSTITUTION =	 {ANSI},
  MONTH =	 feb,
  YEAR =	 {1990},
  ABSTRACT =	 "The Broadband Link-layer Block Transfer protocol,
                  BLINKBLT, is modified to use a cell format based
                  upon the ATM AAL for VBR services. The receiver can
                  distinguish between the two protocols by the value
                  in the AAL length field, which becomes the BLINKBLT
                  PDU type field if higher than 48. This permits the
                  Data Link layer protocol to coexist with the
                  AAL. Optional forward error correction is also
                  added. The modified BLINKBLT is presented. The
                  BLITBLNK cell format is compatible with the proposed
                  AAL for VBR services, but its procedures are
                  different. It features cell-based loss recovery and
                  receiver-paced flow control. It provides
                  conenction-oriented OSI Data Link service in 2
                  sublayers, versus 3 when the AAL is used. Further
                  study of BLINKBLT is proposed as an alternative
                  protocol for high-volume data communications. ",
  TOPIC =	 "AAL",
}

