}

@inproceedings{Kapoor:91,
  AUTHOR =	 "Sanjay Kapoor and Gurudatta M. Parulkar",
  TITLE =	 "{Design of an ATM--FDDI Gateway}",
  BOOKTITLE =	 sigcomm,
  ORGANIZATION = "ACM",
  YEAR =	 1991,
  PAGES =	 "173--182",
  ANNOTE =	 "Proposes VHSI (very high speed internet abstraction)
                  which supports both connection oriented and
                  connectionless. CL traffic is multiplexed at the
                  gateway. The gateway acts as the resource manager of
                  its network. Introduces notion of congram: an almost
                  relable service with no hop-to-hop flow and error
                  control. A user congram (UCon) iis a user set-up
                  connected that is active while the user requires
                  it. A persistent internet congram (PICon) is set up
                  by the network to mux CL traffic. It was previously
                  proposed that FDDI send only asynchronous frames on
                  internet (solving many of our
                  problems). Implementation shown using AMD's
                  SUPERNET. The SAR segments frames with 10 bit
                  sequence number. Adds a 3 byte SAR header to the 5
                  byte ATM header (which does not follow any ATM
                  standard) to include the sequence number, CRC,
                  unused, F (for end-of-frame) and C to distinguish
                  control cells. Design does not implement any
                  explicit rate or congestion control.",
  ABSTRACT =	 "Asynchronous transfer mode (ATM) networks are
                  capable of supporting a wide variety of applications
                  with varying data rates. FDDI networks offer to
                  support similar applications in a LAN
                  environment. Both these networks are characterized
                  by high data rates, low error rates, and are capable
                  of providing performance guarantees. In this paper
                  we present the design of an ATM-FDDI gateway that
                  can provide high performance internetworking between
                  these two important classes of networks. An
                  important part of the gateway design philosophy is
                  to partition the functionality into critical and
                  non-critical paths. The critical path consists of
                  per packet processing and is implemented in
                  hardware. The non-critical path consists of
                  connection, resource, and route management, and is
                  implemented in software.",
  TOPIC =	 "Internet FDDI ATM",
}

