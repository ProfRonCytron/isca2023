@article{Topolcic:90,
  AUTHOR =	 "C. Topolcic",
  TITLE =	 "{Experimental Internet Stream Protocol, Version 2
                  (ST-II)}",
  JOURNAL =	 "Network Information Center RFC 1190",
  MONTH =	 oct,
  YEAR =	 1990,
  PAGES =	 "1--148",
  ABSTRACT =	 "This memo defines the Internet Stream Protocol,
                  Version 2 (ST-II), an IP-layer protocol that
                  provides end-to-end guaranteed service across an
                  internet. This specification obsoletes IEN 119 ``ST-
                  A Proposed Internet Stream Protocol'' written by Jim
                  Forgie in 1979, the previous specifications of
                  ST. ST-II is not compatible with Version 1 of the
                  protocol, but maintains much of the architecture and
                  the philosophy of that version. It is intended to
                  fill in some of the areas left unaddressed, to make
                  it easier to implement, and to support a wider range
                  of applications."
}

@inproceedings{Torek:85,
  AUTHOR =	 {J. O'Toole and C. Torek and M. Weiser},
  TITLE =	 "{Implementing the XNS Protocol for 4.2 BSD}",
  YEAR =	 1985,
  BOOKTITLE =	 {Proceedings of the 1985 Winter USENIX Conference},
  PAGES =	 {90--97},
  ABSTRACT =	 "We have implemented the Xerox Networking Systems
                  protocol suite in 4.2 BSD UNIX in order to
                  communicate with Xerox workstations which do not
                  implement TCP/IP. In this paper, we discuss the
                  problems we encountered while making the necessary
                  changes to the UNIX kernel. We found a multitude of
                  TCP/IP dependencies in the lowest level UNIX device
                  driver and network interface code, which were
                  eliminated by replacing inline code with protocol
                  specific subroutine calls. Another problem was that
                  4.2BSD expects all protocols to be connectionless or
                  byte stream, and the XNS stream level is neither. We
                  generalize the socket code to satisfy the additional
                  requirements of the XNS stream protocol. The result
                  of all these changes is not just the ability to
                  handle XNS but a more general network system within
                  TCP/IP dependencies, ready for the next generation
                  of protocols."
}

